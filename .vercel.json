{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "prisma",
        "use": "@prisma/cli"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  